      "allowJs": true,
      "noImplicitAny": false,
      "removeComments": true,
      "preserveConstEnums": true,
      "outDir": "electron/dist"
  },
  "exclude": [
      "node_modules",
      "client/*",
      "electron/dist/*"
  ]
}
